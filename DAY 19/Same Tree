
class Solution {
public:
    bool isSameTree(TreeNode* p, TreeNode* q) {
        if(p == NULL && q == NULL)
        return true;
        if(!p || !q)
        return false;
        if(p -> val != q -> val)
        return false;

        bool ls = isSameTree(p -> left, q -> left);
        bool rs = isSameTree(p -> right, q -> right);

        return ls && rs;
    }
};
